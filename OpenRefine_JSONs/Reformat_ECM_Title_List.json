[
    {
        "op": "core/column-reorder",
        "columnNames": [
            "Book ID",
            "Shared",
            "Access Model",
            "Quantity"
        ],
        "description": "Remove unneeded columns"
    },
    {
        "op": "core/text-transform",
        "engineConfig": {
            "facets": [],
            "mode": "row-based"
        },
        "columnName": "Book ID",
        "expression": "value.toNumber()",
        "onError": "keep-original",
        "repeat": false,
        "repeatCount": 10,
        "description": "Change values in column ``Book ID`` to numbers"
    },
    {
        "op": "core/text-transform",
        "engineConfig": {
            "facets": [],
            "mode": "record-based"
        },
        "columnName": "Quantity",
        "expression": "value.toNumber()",
        "onError": "keep-original",
        "repeat": false,
        "repeatCount": 10,
        "description": "Change values in column ``Quantity`` to numbers"
    },
    {
        "op": "core/column-addition",
        "engineConfig": {
            "facets": [],
            "mode": "record-based"
        },
        "baseColumnName": "Book ID",
        "expression": "grel:value+\"-\"+cells[\"Access Model\"].value+\"-\"+cells.Shared.value",
        "onError": "set-to-blank",
        "newColumnName": "Dedupe",
        "columnInsertIndex": 0,
        "description": "Create column ``Dedupe`` combining values in ``Book ID``, ``Access Model``, and ``Shared``"
    },
    {
        "op": "core/row-reorder",
        "mode": "record-based",
        "sorting": {
            "criteria": [
                {
                    "valueType": "string",
                    "column": "Dedupe",
                    "blankPosition": 2,
                    "errorPosition": 1,
                    "reverse": false,
                    "caseSensitive": false
                }
            ]
        },
        "description": "Reorder rows based on ``Dedupe``"
    },
    {
        "op": "core/blank-down",
        "engineConfig": {
            "facets": [],
            "mode": "row-based"
        },
        "columnName": "Dedupe",
        "description": "Blank down cells in column ``Dedupe``"
    },
    {
        "op": "core/text-transform",
        "engineConfig": {
            "facets": [
                {
                    "type": "list",
                    "name": "Dedupe",
                    "expression": "isBlank(value)",
                    "columnName": "Dedupe",
                    "invert": false,
                    "omitBlank": false,
                    "omitError": false,
                    "selection": [
                        {
                            "v": {
                                "v": true,
                                "l": "true"
                            }
                        }
                    ],
                    "selectBlank": false,
                    "selectError": false
                }
            ],
            "mode": "record-based"
        },
        "columnName": "Quantity",
        "expression": "grel:row.record.cells[\"Quantity\"].value.sum()",
        "onError": "keep-original",
        "repeat": false,
        "repeatCount": 10,
        "description": "For records representing rows with the same access model for a given title, change the value in ``Quantity`` to the sum of all the ``Quantity`` values for that record"
    },
    {
        "op": "core/row-removal",
        "engineConfig": {
            "facets": [
                {
                    "type": "list",
                    "name": "Dedupe",
                    "expression": "isBlank(value)",
                    "columnName": "Dedupe",
                    "invert": false,
                    "omitBlank": false,
                    "omitError": false,
                    "selection": [
                        {
                            "v": {
                                "v": true,
                                "l": "true"
                            }
                        }
                    ],
                    "selectBlank": false,
                    "selectError": false
                }
            ],
            "mode": "row-based"
        },
        "description": "Remove rows containing duplicate access model statements for a given title"
    },
    {
        "op": "core/column-removal",
        "columnName": "Dedupe",
        "description": "Remove column ``Dedupe``"
    },
    {
        "op": "core/column-addition",
        "engineConfig": {
            "facets": [],
            "mode": "record-based"
        },
        "baseColumnName": "Access Model",
        "expression": "grel:if(value==\"1 User\",1,if(value==\"3 User\",2,if(value.contains(\"Concurrent Access\"),3,4)))",
        "onError": "set-to-blank",
        "newColumnName": "Reorder",
        "columnInsertIndex": 1,
        "description": "Create column ``Reorder`` numbering the order in which the access models should be handled"
    },
    {
        "op": "core/row-reorder",
        "mode": "record-based",
        "sorting": {
            "criteria": [
                {
                    "valueType": "number",
                    "column": "Book ID",
                    "blankPosition": 2,
                    "errorPosition": 1,
                    "reverse": false
                },
                {
                    "valueType": "string",
                    "column": "Shared",
                    "blankPosition": 2,
                    "errorPosition": 1,
                    "reverse": false,
                    "caseSensitive": false
                },
                {
                    "valueType": "number",
                    "column": "Reorder",
                    "blankPosition": 2,
                    "errorPosition": 1,
                    "reverse": false
                }
            ]
        },
        "description": "Reorder rows to put multiple access models for individual titles together in the appropriate order"
    },
    {
        "op": "core/column-removal",
        "columnName": "Reorder",
        "description": "Remove column ``Reorder``"
    },
    {
        "op": "core/column-addition",
        "engineConfig": {
            "facets": [],
            "mode": "record-based"
        },
        "baseColumnName": "Access Model",
        "expression": "grel:if(value==\"1 User\",if(cells.Quantity.value==1,\"1U\",round(cells.Quantity.value)+\"U {1U x\"+round(cells.Quantity.value)+\"}\"),if(value==\"3 User\",if(cells.Quantity.value==1,\"3U\",round(cells.Quantity.value*3)+\"U {3U x\"+round(cells.Quantity.value)+\"}\"),if(value.contains(\"Concurrent Access\"),if(value.split(\" \").length()==2,if(cells.Quantity.value==1,\"NL (??? days)\",\"NL (??? days {quantity \"+round(cells.Quantity.value)+\"})\"),if(cells.Quantity.value==1,\"NL (\"+value.split(\" \")[2].substring(1,-1)+\" days)\",\"NL (\"+round(cells.Quantity.value*value.split(\" \")[2].substring(1,-1).toNumber())+\" days {\"+value.split(\" \")[2].substring(1,-1)+\" days x\"+round(cells.Quantity.value)+\"})\")),if(value.contains(\"Unlimited User\"),if(cells.Quantity.value==1,if(value.contains(\"DRM-free\"),\"UA (Unlimited) DRM-free\",\"UA (Unlimited)\"),if(value.contains(\"DRM-free\"),\"UA (Unlimited) DRM-free {\"+round(cells.Quantity.value)+\"}\",\"UA (Unlimited) {\"+round(cells.Quantity.value)+\"}\")),\"ERROR\"))))+if(cells.Shared.value==\"TRUE\",\" Consortium\",\"\")",
        "onError": "set-to-blank",
        "newColumnName": "User Access Model",
        "columnInsertIndex": 1,
        "description": "Create column ``User Access Model`` with the user access model statement based on the basic access model, the quantity, and if the title is shared"
    },
    {
        "op": "core/column-removal",
        "description": "Remove column ``Shared``",
        "columnName": "Shared"
    },
    {
        "op": "core/column-removal",
        "description": "Remove column ``Access Model``",
        "columnName": "Access Model"
    },
    {
        "op": "core/column-removal",
        "columnName": "Quantity",
        "description": "Remove column ``Quantity``"
    }
]


    {
        "op": "core/blank-down",
        "engineConfig": {
            "facets": [],
            "mode": "row-based"
        },
        "columnName": "Book ID",
        "description": "Blank down cells in column Book ID"
    },
    {
        "op": "core/multivalued-cell-join",
        "columnName": "User Access Model",
        "keyColumnName": "Book ID",
        "separator": "; ",
        "description": "Combine all values in ``User Access Model`` in a record in a single row separated by semicolons"
    }